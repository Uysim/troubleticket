<%= simple_form_for(@trouble) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :client %>
    <%= f.association :user %>
    <%= f.input :range %>
    <%= f.input :state %>
    <%= f.input :detail %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
